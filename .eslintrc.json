  "root": true,
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": ["react", "flowtype"],
  "extends": ["uploadcare", "plugin:react/recommended", "plugin:flowtype/recommended"]
}
